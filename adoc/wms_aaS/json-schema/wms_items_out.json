{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "definitions": {
    "uuid": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "uuid_v1": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-1[0-9a-fA-F]{3}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "item": {
      "type": "object",
      "required": [
        "code",
        "class",
        "occured_at"
      ],
      "additionalProperties": false,
      "properties": {
        "code": {
          "type": "string"
        },
        "class": {
          "$ref": "#/definitions/uuid"
        },
        "occured_at": {
          "type": "integer"
        }
      }
    },
    "error-item": {
      "type": "object",
      "required": [
        "comment",
        "code",
        "occured_at",
        "task_action",
        "problem",
        "class"
      ],
      "additionalProperties": false,
      "properties": {
        "comment": {
          "type": "string",
          "description": "text error message"
        },
        "code": {
          "type": "string",
          "description": "e.g. W0A123456789014"
        },
        "occured_at": {
          "type": "integer",
          "description": "e.g. 1531751799583"
        },
        "task_action": {
          "type": "string",
          "enum": [
            "store",
            "unload",
            "fulfill"
          ]
        },
        "problem": {
          "type": "string",
          "enum": [
            "shortage",
            "excess",
            "defect",
            "underfill",
            "cancellation",
            "not_problem"
          ]
        },
        "class": {
          "$ref": "#/definitions/uuid"
        }
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "generated",
    "serial",
    "uid",
    "userial"
  ],
  "properties": {
    "errors": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/error-item"
      }
    },
    "stored": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/item"
      }
    },
    "unprocessed": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "problems": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "reason",
          "code",
          "error"
        ],
        "additionalProperties": false,
        "properties": {
          "reason": {
            "type": "string"
          },
          "code": {
            "type": "string"
          },
          "error": {
            "type": "integer"
          }
        }
      }
    },
    "revision": {
      "$ref": "#/definitions/uuid_v1"
    },
    "revocation": {
      "type": "object",
      "required": [
        "owner",
        "origin",
        "occured_at"
      ],
      "additionalProperties": false,
      "properties": {
        "owner": {
          "$ref": "#/definitions/uuid"
        },
        "origin": {
          "$ref": "#/definitions/uuid"
        },
        "occured_at": {
          "type": "integer",
          "description": "e.g. 1531751799583"
        }
      }
    },
    "uid": {
      "$ref": "#/definitions/uuid"
    },
    "userial": {
      "$ref": "#/definitions/uuid_v1"
    },
    "generated": {
      "type": "integer",
      "description": "timestamp e.g. 1531752070460",
      "minimum": 0
    },
    "final": {
      "type": "boolean"
    },
    "not-in-stock": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "serial": {
      "type": "integer",
      "description": "timestamp e.g. 145481128145898",
      "minimum": 0
    },
    "total": {
      "type": "integer"
    },
    "success": {
      "type": "boolean"
    },
    "fulfilled": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/item"
      }
    },
    "unloaded": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/item"
      }
    },
    "divergence": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/error-item"
      }
    }
  }
}